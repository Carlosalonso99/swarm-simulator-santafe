<% for y in 0..1 %>
  <% for x in 0..8 %>
    <model name="building_group1_<%=y*8 + x%>">
    <pose><%=0 - x * 170%> <%=1535 + 400*y%> 0 0 0 0</pose>
      <static>true</static>
      <link name="link">
        <visual name="visual">
          <geometry>
            <box>
              <size>150 350 50</size>
            </box>
          </geometry>
          <material>
            <ambient>0.2 .2 .2 0.2</ambient>
            <diffuse>0.2 .2 .2 0.2</diffuse>
          </material>
        </visual>

        <collision name="collision">
          <geometry>
            <box>
              <size>150 350 50</size>
            </box>
          </geometry>
        </collision>
      </link>
    </model>
  <%end%>
<%end%>

<% for y in 0..1 %>
  <% for x in 0..8 %>
    <model name="building_group2_<%=y*10 + x%>">
    <pose><%=1800 + x * 170%> <%=-1030 - 400*y%> 0 0 0 0</pose>
      <static>true</static>
      <link name="link">
        <visual name="visual">
          <geometry>
            <box>
              <size>150 350 100</size>
            </box>
          </geometry>
          <material>
            <ambient>0.2 .2 .2 0.2</ambient>
            <diffuse>0.2 .2 .2 0.2</diffuse>
          </material>
        </visual>

        <collision name="collision">
          <geometry>
            <box>
              <size>150 350 80</size>
            </box>
          </geometry>
        </collision>
      </link>
    </model>
  <%end%>
<%end%>
