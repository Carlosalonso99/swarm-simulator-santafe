#!/usr/bin/gnuplot

reset
set terminal png

set xdata time
#set timefmt "%d/%m/%Y %H:%M:%S"
#set format x "%H:%M"
set xlabel "time"

set ylabel "Messages"

set title "Messages sent"
set key reverse Left outside
set grid

set style data linespoints
set datafile separator ","

plot 'swarm.csv' using 1:2 with lines
